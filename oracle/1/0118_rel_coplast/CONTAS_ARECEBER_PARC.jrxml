	<queryString>
		<![CDATA[SELECT FIN.NUFIN
, FIN.NUMNOTA
, FIN.DESDOBRAMENTO
, FIN.DTVENC
, FIN.DTNEG
, FIN.DHBAIXA
, FIN.VLRDESDOB
, FIN.VLRDESC
, FIN.VLRJURO
, FIN.CODPARC
, PAR.RAZAOSOCIAL
, FIN.CODEMP
, EMP.RAZAOSOCIAL AS NOMEEMPRESA
, (CAST(FIN.CODPARC AS VARCHAR) + ' - ' + PAR.RAZAOSOCIAL) AS CLIENTE
, FIN.HISTORICO
, (CAST(FIN.NUMNOTA AS VARCHAR) + '-' + FIN.DESDOBRAMENTO) AS NRODOC
FROM TGFFIN FIN
, TGFPAR PAR
, TSIEMP EMP
WHERE FIN.CODPARC = PAR.CODPARC
AND FIN.CODEMP = EMP.CODEMP
AND FIN.RECDESP = 1
AND FIN.PROVISAO = 'N'
AND FIN.DHBAIXA IS NULL
AND FIN.CODNAT = '10304000'
AND cast(FIN.DTVENC as date)>=cast($P{dtini} as date)
and cast(FIN.DTVENC as date)<=cast($P{dtfin} as date)
AND FIN.CODEMP=ISNULL($P{codemp},FIN.CODEMP)
AND FIN.CODPARC=ISNULL($P{codparc},FIN.CODPARC)
ORDER BY CODPARC]]>
	</queryString>
