	<queryString>
		<![CDATA[SELECT
FIN.CODPARC,
PAR.NOMEPARC,
PAR.RAZAOSOCIAL,
PAR.CODPARCMATRIZ,
FIN.NUFIN,
NVL(FIN.NUNOTA,0),
FIN.CODEMP,
EMP.NOMEFANTASIA,
FIN.VLRDESDOB,
FIN.DTVENC,
FIN.DHBAIXA,
CASE WHEN FIN.RECDESP = 1 THEN 'RECEITA'
     WHEN FIN.RECDESP = -1 THEN 'DESPESA'
     WHEN FIN.RECDESP = 0 THEN 'OUTROS'
END AS MOVIMENTO,
CASE WHEN FIN.PROVISAO = 'S' THEN 'Sim'
     WHEN FIN.PROVISAO = 'N' THEN 'N찾o'
END AS PROVISAO,

FIN.VLRBAIXA

FROM TGFFIN FIN
INNER JOIN TGFPAR PAR ON FIN.CODPARC = PAR.CODPARC
INNER JOIN TSIEMP EMP ON FIN.CODEMP = EMP.CODEMP

WHERE FIN.DHBAIXA = TRUNC(SYSDATE)
AND FIN.PROVISAO = 'N'
ORDER BY FIN.RECDESP]]>
	</queryString>
