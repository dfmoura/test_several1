	</parameter>
	<parameter name="P_IMANAL" class="java.lang.String">
		<property name="NOMECAMPO" value="TGFFIN.PROVISAO"/>
		<parameterDescription><![CDATA[Gerar Analiticos]]></parameterDescription>
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_CODFECH" class="java.lang.String">
		<property name="NOMETABELA" value="AD_FECHAPLUS"/>
		<parameterDescription><![CDATA[Fechamento Plus]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    ANL.CODFECH,
    ANL.AGRUPADOR,
    ANL.CODPARC || ' - ' || (SELECT RAZAOSOCIAL FROM TGFPAR WHERE CODPARC = ANL.CODPARC) AS PARCEIRO,
    ANL.CODGAT,
    SIT.CODGAT AS GAT2,
    ANL.CODVEND,
    ANL.NUMNOTA,
    ANL.NUNOTA,
    (SELECT AD_REFFORCA FROM TGFCAB WHERE NUNOTA = ANL.NUNOTA) AS REF_A2W,
    ANL.DTNEG,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.QTDNEG ELSE ANL.QTDNEG END) AS QTDNEG,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.QTDNEGVOL ELSE ANL.QTDNEGVOL END) AS QTDNEGVOL,
    SUM(ANL.REALIZADO) AS REALIZADO,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.VLRICMS ELSE ANL.VLRICMS END) AS VLRICMS,
    SUM(ANL.REALIZADO) - SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.VLRICMS ELSE ANL.VLRICMS END) AS VLRLIQ,
    NVL(MAX(SIT.BASECALC), 0) AS BASE_BENE,
CASE WHEN SUM(ANL.REALIZADO) < 0 THEN 'Devolução'  WHEN SUM(ANL.REALIZADO) > 0 THEN 'Venda' END AS TIPONOT
FROM
    AD_ADANALIPLUS ANL
LEFT JOIN
    AD_REALSINTET SIT
    ON ANL.CODFECH = SIT.CODFECH
    AND SIT.AGRUPADOR = 'GATILHO'
WHERE
    ANL.AGRUPADOR = 'GATILHO'


  AND (
        ($P{P_IMANAL} = 'N' AND SIT.CODVEND IS NULL)
     OR (($P{P_IMANAL} = 'S' OR $P{P_IMANAL} IS NULL) AND SIT.CODVEND = $P{P_CODVEND})
      )



    AND (
        (SIT.CODGAT = 2)  -- traz todos os vendedores
        OR
        (SIT.CODGAT = 1 AND ANL.CODVEND = SIT.CODVEND)
    )
AND SIT.CODFECH = $P{P_CODFECH}
GROUP BY
    ANL.CODFECH,
    ANL.AGRUPADOR,
    ANL.CODGAT,
    ANL.NUMNOTA,
    ANL.NUNOTA,
    ANL.CODVEND,
    ANL.DTNEG,
    ANL.CODPARC,
    SIT.CODGAT
ORDER BY ANL.DTNEG]]>
	</queryString>
