	<queryString>
		<![CDATA[SELECT CAB.NUNOTA
, CAB.NUMNOTA
, CAB.OBSERVACAO
, EMP.RAZAOSOCIAL AS RAZAOEMP
, CAB.CODVEND
, VEN.APELIDO
, CAB.CODPARC
, PAR.NOMEPARC
, PAR.RAZAOSOCIAL
, PAR.TELEFONE
, PAR.FAX
, PAR.CGC_CPF
, PAR.CEP
, PAR.INSCESTADNAUF
, ENDE.NOMEEND
, PAR.NUMEND
, CID.NOMECID
, UFS.DESCRICAO
, UFS.UF
, BAI.NOMEBAI
, CAB.VLRNOTA
, CAB.VLRDESCSERV
, CAB.VLRDESCTOT
, CAB.VLRDESCTOTITEM
, CIF_FOB||'-'||(SELECT OPC.OPCAO FROM TDDOPC OPC JOIN TDDCAM CAM ON OPC.NUCAMPO = CAM.NUCAMPO WHERE CAM.NOMETAB = 'TGFCAB' AND CAM.NOMECAMPO = 'CIF_FOB' AND OPC.VALOR = CAB.CIF_FOB) CIF_FOB
, CASE WHEN CAB.CODPARCTRANSP = 1 THEN 'APPC' ELSE 'TERCEIRO' END TRANSPORTADOR
, CAB.AD_DTCARREGAMENTO
, CAB.AD_DTENTREGA
FROM TGFCAB CAB
, TGFPAR PAR
, TSIEND ENDE
, TSIUFS UFS
, TSICID CID
, TSIBAI BAI
, TGFVEI VEI
, TSIEMP EMP
, TGFVEN VEN
WHERE CAB.CODPARC = PAR.CODPARC
AND PAR.CODEND = ENDE.CODEND
AND PAR.CODCID = CID.CODCID
AND CID.UF = UFS.CODUF
AND PAR.CODBAI= BAI.CODBAI
AND CAB.CODVEICULO = VEI.CODVEICULO
AND CAB.CODEMP = EMP.CODEMP
AND VEN.CODVEND = CAB.CODVEND
AND CAB.NUNOTA = $P{NUNOTA}
ORDER BY CAB.DTNEG, CAB.NUNOTA, CAB.CODEMP]]>
