	<queryString>
		<![CDATA[SELECT CAB.NUNOTA
, CAB.NUMNOTA
, EMP.RAZAOSOCIAL AS RAZAOEMP
, CAB.CODVEND
, VEN.APELIDO
, CAB.CODPARC
, PAR.NOMEPARC
, PAR.RAZAOSOCIAL
, PAR.TELEFONE
, PAR.FAX
, PAR.CGC_CPF
, PAR.CEP
, PAR.IDENTINSCESTAD
, ENDE.NOMEEND
, PAR.NUMEND
, CID.NOMECID
, UFS.DESCRICAO
, UFS.UF
, BAI.NOMEBAI
, CAB.VLRNOTA
, CAB.VLRDESCSERV
, CAB.VLRDESCTOT +  CAB.VLRDESCTOTITEM AS VLRDESCTOT 
, CAB.VLRDESCTOTITEM
, UPPER(TOP.DESCROPER) DESCROPER
, CAB.AD_INFOADICIONAIS
FROM TGFCAB CAB
, TGFTOP TOP
, TGFPAR PAR
, TSIEND ENDE
, TSIUFS UFS
, TSICID CID
, TSIBAI BAI
, TGFVEI VEI
, TSIEMP EMP
, TGFVEN VEN
WHERE CAB.CODPARC = PAR.CODPARC
AND CAB.CODTIPOPER = TOP.CODTIPOPER
AND CAB.DHTIPOPER = TOP.DHALTER
AND PAR.CODEND = ENDE.CODEND
AND PAR.CODCID = CID.CODCID
AND CID.UF = UFS.CODUF
AND PAR.CODBAI= BAI.CODBAI
AND CAB.CODVEICULO = VEI.CODVEICULO
AND CAB.CODEMP = EMP.CODEMP
AND VEN.CODVEND = CAB.CODVEND
AND CAB.NUNOTA = $P{NUNOTA}
ORDER BY CAB.DTNEG, CAB.NUNOTA, CAB.CODEMP]]>
	</queryString>
