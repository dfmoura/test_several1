	<queryString>
		<![CDATA[SELECT
FIN.CODEMP,
FIN.CODPARC,
PAR.NOMEPARC,
SANKHYA.fn_FormCPF_CNPJ(PAR.CGC_CPF)CGC_CPF,
SUM(FIN.VLRDESDOB) RECABE
FROM TGFFIN FIN
INNER JOIN TGFPAR PAR ON FIN.CODPARC = PAR.CODPARC
WHERE
FIN.RECDESP = 1
AND FIN.DTNEG <= $P{DTREFERENCIA}
AND	(FIN.DHBAIXA > $P{DTREFERENCIA} OR FIN.DHBAIXA IS NULL)

AND (PAR.CODPARC = $P{P_CODPARC} OR $P{P_CODPARC} IS NULL)
AND FIN.PROVISAO = 'N'
AND FIN.CODEMP = $P{CODEMP}
GROUP BY
FIN.CODEMP,
FIN.CODPARC,
PAR.NOMEPARC,
PAR.CGC_CPF]]>
