	<queryString>
		<![CDATA[WITH PARCEIROS AS (
    SELECT DISTINCT
        CAB.NUNOTA AS NUNICO,
        CAB.CODPARC,
        DECODE(NVL(PAR.NOMEPARC, '<SEM PARCEIRO>'), '<SEM PARCEIRO>', 'SEM REGISTRO', PAR.NOMEPARC) AS NOMEPARC
    FROM TGFCAB CAB
    INNER JOIN TCBINT IT ON CAB.NUNOTA = IT.NUNICO AND IT.ORIGEM = 'E'
    LEFT JOIN TGFPAR PAR ON CAB.CODPARC = PAR.CODPARC
    WHERE TRUNC(CAB.DTNEG, 'MM') IN (TRUNC(ADD_MONTHS($P{DTREFERENCIA}, -1), 'MM'), $P{DTREFERENCIA})

    UNION ALL

    SELECT DISTINCT
        FIN.NUFIN AS NUNICO,
        FIN.CODPARC,
        DECODE(NVL(PAR.NOMEPARC, '<SEM PARCEIRO>'), '<SEM PARCEIRO>', 'SEM REGISTRO', PAR.NOMEPARC) AS NOMEPARC
    FROM TGFFIN FIN
    INNER JOIN TCBINT IT ON FIN.NUFIN = IT.NUNICO AND IT.ORIGEM IN ('F','B','R')
    LEFT JOIN TGFPAR PAR ON FIN.CODPARC = PAR.CODPARC
    WHERE TRUNC(FIN.DTNEG, 'MM') IN (TRUNC(ADD_MONTHS($P{DTREFERENCIA}, -1), 'MM'), $P{DTREFERENCIA})
),

LANCAMENTOS AS (
    SELECT
        LAN.CODEMP,
        LAN.CODCTACTB,
        PLA.CTACTB,
        PLA.DESCRCTA,
        NVL(P.CODPARC, 0) AS CODPARC,
        NVL(P.NOMEPARC, 'SEM REGISTRO') AS NOMEPARC,
        LAN.REFERENCIA,
        CASE WHEN LAN.REFERENCIA = TRUNC(ADD_MONTHS($P{DTREFERENCIA}, -1), 'MM') THEN LAN.VLRLANC ELSE 0 END * DECODE(LAN.TIPLANC, 'D', 1, 'R', -1, 0) AS SALDOANT,
        CASE WHEN LAN.REFERENCIA = $P{DTREFERENCIA} AND LAN.TIPLANC = 'D' THEN LAN.VLRLANC ELSE 0 END AS DEBITO,
        CASE WHEN LAN.REFERENCIA = $P{DTREFERENCIA} AND LAN.TIPLANC = 'R' THEN LAN.VLRLANC * -1 ELSE 0 END AS CREDITO
    FROM TCBLAN LAN
    INNER JOIN TCBPLA PLA ON PLA.CODCTACTB = LAN.CODCTACTB
    LEFT JOIN TCBINT IT ON (
        IT.CODEMP = LAN.CODEMP AND
        IT.REFERENCIA = LAN.REFERENCIA AND
        IT.NUMLANC = LAN.NUMLANC AND
        IT.TIPLANC = LAN.TIPLANC AND
        IT.NUMLOTE = LAN.NUMLOTE AND
        IT.SEQUENCIA = LAN.SEQUENCIA
    )
    LEFT JOIN PARCEIROS P ON IT.NUNICO = P.NUNICO
    WHERE LAN.CODEMP = $P{CODEMP}
      AND LAN.CODCTACTB = $P{CODCTACTB}
      AND LAN.REFERENCIA IN (TRUNC(ADD_MONTHS($P{DTREFERENCIA}, -1), 'MM'), $P{DTREFERENCIA})
)

SELECT
    CODEMP,
    CODPARC,
    NOMEPARC,
    CODCTACTB,
    CTACTB,
    DESCRCTA,
    SUM(SALDOANT) AS SALDOANT,
    SUM(DEBITO) AS DEBITO,
    SUM(CREDITO) AS CREDITO,
    SUM(DEBITO + CREDITO) AS SALDOATUAL,
    CASE
        WHEN SUM(SALDOANT) <> 0 THEN
            CASE
                WHEN CTACTB < '2' THEN CASE WHEN SUM(SALDOANT) >= 0 THEN 'D' ELSE 'C' END
                WHEN CTACTB >= '2' AND CTACTB < '3' THEN CASE WHEN SUM(SALDOANT) <= 0 THEN 'C' ELSE 'D' END
                ELSE 'D'
            END
        ELSE
            CASE
                WHEN CTACTB < '2' THEN CASE WHEN SUM(DEBITO) + SUM(CREDITO) >= 0 THEN 'D' ELSE 'C' END
                WHEN CTACTB >= '2' AND CTACTB < '3' THEN CASE WHEN SUM(DEBITO) + SUM(CREDITO) <= 0 THEN 'C' ELSE 'D' END
                ELSE 'D'
            END
    END AS DC
FROM LANCAMENTOS
WHERE ($P{P_CODPARC} IS NULL OR CODPARC = $P{P_CODPARC})
GROUP BY
    CODEMP,
    CODPARC,
    NOMEPARC,
    CODCTACTB,
    CTACTB,
    DESCRCTA
ORDER BY
    CODEMP,
    CODPARC]]>
	</queryString>
