	<queryString>
		<![CDATA[SELECT
     SIN.CODFECH,
     FEC.INISAFRA,
     FEC.FINSAFRA,
     FEC.DESCR,
     SIN.CODVEND,
     (SELECT APELIDO FROM TGFVEN WHERE CODVEND = SIN.CODVEND) AS APELIDO,
     GAT.DESCRGAT,
     NVL(SIN.META,0) AS META,
     NVL(SIN.REAL,0) AS REAL,
     NVL(SIN.PERCREAL,0) AS PERCREAL,
     SIN.FORMULA,
     SIN.BASECALC,
     VAR.DESCRVAR,
    -- Tendência projetada (valor até o fim da safra OU realizado se já passou)


    CASE
        WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) THEN SIN.REAL
        WHEN (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0
        THEN ROUND((SIN.REAL / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) * (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA)), 2)
        ELSE 0
    END AS TENDENCIA_REAL,

    -- Tendência projetada percentual sobre a meta
  SIN.CODVAR,

CASE
    WHEN SIN.CODVAR = 2 THEN
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0
                 AND PMED.META > 0 THEN
                ROUND(
                    ((SIN.REAL / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) / PMED.META * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) AND PMED.META > 0 THEN
                ROUND(PMED.REAL / PMED.META * 100, 2)
            ELSE 0
        END

    WHEN SIN.CODVAR = 1 THEN
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0 THEN
                ROUND(
                    ((SIN.REALFIN / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) /
                    NULLIF((SELECT REALFIN
                            FROM AD_REALSINTET
                            WHERE CODVEND = SIN.CODVEND
                              AND AGRUPADOR = 'GATILHO'
                              AND ROWNUM = 1), 0) * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) THEN
                ROUND(
                    SIN.REALFIN /
                    NULLIF((SELECT REALFIN
                            FROM AD_REALSINTET
                            WHERE CODVEND = SIN.CODVEND
                              AND AGRUPADOR = 'GATILHO'
                              AND ROWNUM = 1), 0) * 100, 2)
            ELSE 0
        END

    ELSE
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0
                 AND SIN.META > 0 THEN
                ROUND(
                    ((SIN.REAL / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) / SIN.META * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) AND SIN.META > 0 THEN
                ROUND(SIN.REAL / SIN.META * 100, 2)
            ELSE 0
        END
END AS TENDENCIA_PERC,

CASE WHEN (CASE
    WHEN SIN.CODVAR = 2 THEN
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0
                 AND PMED.META > 0 THEN
                ROUND(
                    ((SIN.REAL / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) / PMED.META * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) AND PMED.META > 0 THEN
                ROUND(PMED.REAL / PMED.META * 100, 2)
            ELSE 0
        END

    WHEN SIN.CODVAR = 1 THEN
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0 THEN
                ROUND(
                    ((SIN.REALFIN / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) /
                    NULLIF((SELECT REALFIN
                            FROM AD_REALSINTET
                            WHERE CODVEND = SIN.CODVEND
                              AND AGRUPADOR = 'GATILHO'
                              AND ROWNUM = 1), 0) * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) THEN
                ROUND(
                    SIN.REALFIN /
                    NULLIF((SELECT REALFIN
                            FROM AD_REALSINTET
                            WHERE CODVEND = SIN.CODVEND
                              AND AGRUPADOR = 'GATILHO'
                              AND ROWNUM = 1), 0) * 100, 2)
            ELSE 0
        END

    ELSE
        CASE
            WHEN TRUNC(SYSDATE) <= TRUNC(FEC.FINSAFRA)
                 AND (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA)) > 0
                 AND SIN.META > 0 THEN
                ROUND(
                    ((SIN.REAL / (TRUNC(SYSDATE) - TRUNC(FEC.INISAFRA))) *
                     (TRUNC(FEC.FINSAFRA) - TRUNC(FEC.INISAFRA))) / SIN.META * 100, 2)
            WHEN TRUNC(SYSDATE) > TRUNC(FEC.FINSAFRA) AND SIN.META > 0 THEN
                ROUND(SIN.REAL / SIN.META * 100, 2)
            ELSE 0
        END
END) >= VVEN.META THEN (SELECT BASECALC FROM AD_REALSINTET WHERE CODVEND = SIN.CODVEND AND AGRUPADOR = 'GATILHO' AND CODFECH = SIN.CODFECH) * (SIN.INDICE/100)

ELSE SIN.BASECALC END AS TENDEBENE,

SIN.INDICE,

BS.BASEBEN,

(SIN.INDICE/100) * BS.BASEBEN AS BENEVAR

FROM
    AD_REALSINTET SIN
LEFT JOIN
    AD_FECHAPLUS FEC ON SIN.CODFECH = FEC.CODFECH
LEFT JOIN
    AD_GATILHOSPLUS GAT ON SIN.CODGAT = GAT.NUCHAVE
LEFT JOIN
    AD_VARIAVPLUS VAR ON SIN.CODVAR = VAR.NUVARIAV
LEFT JOIN
    AD_VARIAVEND VVEN ON SIN.CODVEND = VVEN.CODVEND AND SIN.CODVAR = VVEN.CODVAR
LEFT JOIN

        (
            SELECT
             2 AS CODVAR,
             CODVEND,
             AVG(NVL(PRECOTAB,VLRUNIT)) AS META,
             AVG(PRECOLIQ) AS REAL,
             CODFECH

             FROM

             AD_ADANALIPLUS

             WHERE AGRUPADOR = 'PRECO_MEDIO'
             AND CODVAR = 2

             GROUP BY CODVEND,CODFECH
        )PMED
    ON SIN.CODVEND = PMED.CODVEND AND SIN.CODFECH = PMED.CODFECH AND SIN.CODVAR = PMED.CODVAR

LEFT JOIN (
            SELECT
              SIN.CODVEND,
              SIN.CODFECH,
              CASE
                WHEN GAT.TIPCOMPOS = 1 THEN (VLRFIXO/100)*(SIN.REALFIN - (CASE WHEN (SELECT DESCIMPOSTO FROM AD_GATILHOSPLUS WHERE NUCHAVE = GVEN.CODGAT)  = 'S' THEN SIN.VLRICMS ELSE 0 END))
                WHEN GAT.TIPCOMPOS = 2 THEN (VLRFIXO/100)*REM.VLRREMFIXA
                WHEN GAT.TIPCOMPOS = 3 THEN (((SELECT CASE WHEN PROPORCIAL = 'S' THEN SIN.PERCREAL ELSE RESULTADO END AS RESULTADO FROM AD_GATILHOAPUR WHERE SIN.PERCREAL BETWEEN MINIMA AND MAXIMA AND NUCHAVE = GVEN.CODGAT)/100)*(SIN.REALFIN - (CASE WHEN (SELECT DESCIMPOSTO FROM AD_GATILHOSPLUS WHERE NUCHAVE = GVEN.CODGAT)  = 'S' THEN SIN.VLRICMS ELSE 0 END))  )
                WHEN GAT.TIPCOMPOS = 4 THEN (((SELECT CASE WHEN PROPORCIAL = 'S' THEN SIN.PERCREAL ELSE RESULTADO END AS RESULTADO FROM AD_GATILHOAPUR WHERE SIN.PERCREAL BETWEEN MINIMA AND MAXIMA AND NUCHAVE = GVEN.CODGAT)/100)*REM.VLRREMFIXA )
              ELSE 0
              END AS BASEBEN

            FROM AD_REALSINTET SIN
            LEFT JOIN AD_GATILHOVEND GVEN ON SIN.CODVEND = GVEN.CODVEND
            LEFT JOIN AD_FECHAPLUS FEC ON SIN.CODFECH = FEC.CODFECH
            LEFT JOIN AD_GATILHOSPLUS GAT ON GVEN.CODGAT = GAT.NUCHAVE
            LEFT JOIN AD_VENDSPART REM ON SIN.CODVEND = REM.CODVEND
           WHERE SIN.AGRUPADOR = 'GATILHO'
         )BS ON SIN.CODVEND = BS.CODVEND AND SIN.CODFECH = BS.CODFECH

WHERE
    SIN.AGRUPADOR <> 'GATILHO'
AND
    SIN.CODVEND = $P{P_CODVEND}
AND SIN.CODFECH = $P{P_CODFECH}]]>
	</queryString>
