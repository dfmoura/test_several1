		<parameterDescription><![CDATA[Fechamento Plus]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    CASE WHEN ANL.AGRUPADOR = 'TRABALHO_ENTR' THEN 'Trabalhos Entregues'  WHEN SIT.AGRUPADOR = 'TRABALHO_DESVOL' THEN 'Trabalhos Desenvolvidos' END AS GRUP,
    ANL.CODVAR,
    ANL.CODFECH,
    ANL.AGRUPADOR,
    ANL.CODVEND || ' - ' || (SELECT RAZAOSOCIAL FROM TGFPAR WHERE CODPARC = (SELECT CODPARC FROM TGFVEN WHERE CODVEND = ANL.CODVEND)) AS PARCEIRO,
    ANL.CODGAT,
    SIT.CODGAT AS GAT2,
    ANL.CODVEND,
    ANL.NUMNOTA,
    ANL.NUNOTA,
    (SELECT AD_REFFORCA FROM TGFCAB WHERE NUNOTA = ANL.NUNOTA) AS REF_A2W,
    ANL.DTNEG,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.QTDNEG ELSE ANL.QTDNEG END) AS QTDNEG,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.QTDNEGVOL ELSE ANL.QTDNEGVOL END) AS QTDNEGVOL,
    SUM(ANL.REALIZADO) AS REALIZADO,
    SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.VLRICMS ELSE ANL.VLRICMS END) AS VLRICMS,
    SUM(ANL.REALIZADO) - SUM(CASE WHEN ANL.REALIZADO < 0 THEN -ANL.VLRICMS ELSE ANL.VLRICMS END) AS VLRLIQ,
    NVL(MAX(SIT.BASECALC), 0) AS BASE_BENE
FROM
    AD_ADANALIPLUS ANL
LEFT JOIN
    AD_REALSINTET SIT
    ON ANL.CODFECH = SIT.CODFECH AND ANL.CODVAR = SIT.CODVAR AND ANL.CODVEND = SIT.CODVEND

WHERE
(
        ($P{P_IMANAL} = 'N' AND ANL.CODVEND IS NULL)
     OR (($P{P_IMANAL} = 'S' OR $P{P_IMANAL} IS NULL) AND SIT.CODVEND = $P{P_CODVEND})
      )
AND ANL.CODVAR IN (4,5)
AND SIT.CODFECH = $P{P_CODFECH}

GROUP BY
    SIT.AGRUPADOR,
    ANL.CODFECH,
    ANL.AGRUPADOR,
    ANL.CODGAT,
    ANL.NUMNOTA,
    ANL.NUNOTA,
    ANL.CODVEND,
    ANL.DTNEG,
    ANL.CODPARC,
    SIT.CODGAT,
    ANL.CODVAR]]>
	</queryString>
