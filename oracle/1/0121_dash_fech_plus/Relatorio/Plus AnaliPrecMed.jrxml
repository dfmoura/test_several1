	<queryString>
		<![CDATA[SELECT
   'Preço Medio' AS GRUP,
    ANL.CODVAR,
    ANL.CODFECH,
    ANL.AGRUPADOR,
    ANL.CODPARC || ' - ' || (SELECT RAZAOSOCIAL FROM TGFPAR WHERE CODPARC = ANL.CODPARC) AS PARCEIRO,
    ANL.CODVEND,
    ANL.NUMNOTA,
    ANL.NUNOTA,
    (SELECT AD_REFFORCA FROM TGFCAB WHERE NUNOTA = ANL.NUNOTA) AS REF_A2W,
    ANL.CODPROD||'- '||(SELECT DESCRPROD FROM TGFPRO WHERE CODPROD = ANL.CODPROD)AS PRODUTO ,
    ANL.DTNEG,
    ANL.QTDNEG,
    ANL.QTDNEGVOL,
    ANL.REALIZADO,
    ANL.VLRICMS,
    NVL(ANL.PRECOTAB,ANL.VLRUNIT) AS PRECOTAB,
    ANL.PRECOLIQ AS VLRLIQ,
    ANL.VLRUNIT,
    PMED.META AS PRECO_MEDTABN,
    PMED.REAL AS PRECO_MEDREAL,
    (PMED.REAL/PMED.META) *100 AS PERCMED

FROM
    AD_ADANALIPLUS ANL
LEFT JOIN
    AD_REALSINTET SIT
    ON ANL.CODFECH = SIT.CODFECH AND ANL.CODVAR = SIT.CODVAR AND ANL.CODVEND = SIT.CODVEND
LEFT JOIN

        (
            SELECT
             2 AS CODVAR,
             CODVEND,
             AVG(NVL(PRECOTAB,VLRUNIT)) AS META,
             AVG(PRECOLIQ) AS REAL,
             CODFECH

             FROM

             AD_ADANALIPLUS

             WHERE AGRUPADOR = 'PRECO_MEDIO'
             AND CODVAR = 2

             GROUP BY CODVEND,CODFECH
        )PMED
    ON SIT.CODVEND = PMED.CODVEND AND SIT.CODFECH = PMED.CODFECH AND SIT.CODVAR = PMED.CODVAR


WHERE (
        ($P{P_IMANAL} = 'N' AND ANL.CODVEND IS NULL)
     OR (($P{P_IMANAL} = 'S' OR $P{P_IMANAL} IS NULL) AND SIT.CODVEND = $P{P_CODVEND})
      )

AND ANL.CODVAR  IN (2)
AND SIT.CODFECH = $P{P_CODFECH}
ORDER BY ANL.DTNEG]]>
	</queryString>
