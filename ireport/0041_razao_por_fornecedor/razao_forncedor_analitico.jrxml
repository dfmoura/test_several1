	<subDataset name="Table Dataset 1">
		<queryString>
			<![CDATA[/* Razão Auxilar das Contas que quebra por AD_CODPARC */

SELECT
CODEMP,
REFERENCIA,
NUMLOTE,
NUMLANC,
TIPLANC,
CODPARC,
NOMEPARC,
CODCTACTB,
CTACTB,
DESCRCTA,
DEBITO,
CREDITO,
DTMOV,
CODHISTCTB,
COMPLHIST,
NUMDOC,
VENCIMENTO,
LIBERADO,
CODUSU,
CODPROJ,
SEQUENCIA


FROM (


SELECT	P.CODEMP,
        P.REFERENCIA,
        P.NUMLOTE,
        P.NUMLANC,
        P.TIPLANC,
		P.CODPARC,
		P.NOMEPARC,
		P.CODCTACTB,
		P.CTACTB,
		P.DESCRCTA,
		CASE WHEN SUM(P.DEBITO) < 0 THEN SUM(P.DEBITO)*-1 ELSE SUM(P.DEBITO) END AS DEBITO,
		CASE WHEN SUM(P.CREDITO) < 0 THEN SUM(P.CREDITO)*-1 ELSE SUM(P.CREDITO) END AS CREDITO,

        P.DTMOV,
        P.CODHISTCTB,
        P.COMPLHIST,
        P.NUMDOC,
        P.VENCIMENTO,
        P.LIBERADO,
        P.CODUSU,
        P.CODPROJ,
        P.SEQUENCIA

FROM

(

SELECT
    LAN.CODEMP,
    LAN.REFERENCIA,
    LAN.NUMLOTE,
    LAN.NUMLANC,
    LAN.TIPLANC,
    LAN.CODCTACTB,
    PLA.CTACTB,
    PLA.DESCRCTA,
    PAR.CODPARC,
    PAR.NOMEPARC,


    0 AS DEBITO,
    0 AS CREDITO,
    LAN.DTMOV,
    LAN.CODHISTCTB,
    LAN.COMPLHIST,
    LAN.NUMDOC,
    LAN.VENCIMENTO,
    LAN.LIBERADO,
    LAN.CODUSU,
    LAN.CODPROJ,
    LAN.SEQUENCIA

FROM TCBLAN LAN

INNER JOIN TCBPLA PLA ON PLA.CODCTACTB = LAN.CODCTACTB


    -- JOINs adicionais
    LEFT JOIN TCBINT IT ON (
        IT.CODEMP = LAN.CODEMP AND
        IT.REFERENCIA = LAN.REFERENCIA AND
        IT.NUMLANC = LAN.NUMLANC AND
        IT.TIPLANC = LAN.TIPLANC AND
        IT.NUMLOTE = LAN.NUMLOTE AND
        IT.SEQUENCIA = LAN.SEQUENCIA
    )

    LEFT JOIN TGFCAB CAB ON (
        CAB.NUNOTA = IT.NUNICO AND
        IT.ORIGEM = 'E'
    )

    LEFT JOIN TGFPAR PAR ON (
        PAR.CODPARC = CAB.CODPARC
    )


WHERE
    LAN.REFERENCIA < TO_DATE('01/03/2025', 'DD/MM/YYYY') --:DTREFERENCIA
    AND LAN.CODEMP = 1 --:CODEMP
    AND LAN.CODCTACTB = 128 --:CODCTACTB
    AND PLA.CTACTB >= '1'
    AND PLA.CTACTB < '2'

GROUP BY
    LAN.CODEMP,
    LAN.REFERENCIA,
    LAN.NUMLOTE,
    LAN.NUMLANC,
    LAN.TIPLANC,
    LAN.CODCTACTB,
    PLA.CTACTB,
    PLA.DESCRCTA,
    PAR.CODPARC,
    PAR.NOMEPARC,
    LAN.DTMOV,
    LAN.CODHISTCTB,
    LAN.COMPLHIST,
    LAN.NUMDOC,
    LAN.VENCIMENTO,
    LAN.LIBERADO,
    LAN.CODUSU,
    LAN.CODPROJ,
    LAN.SEQUENCIA


UNION ALL


SELECT	LAN.CODEMP,
        LAN.REFERENCIA,
        LAN.NUMLOTE,
        LAN.NUMLANC,
        LAN.TIPLANC,
		LAN.CODCTACTB,
		PLA.CTACTB,
		PLA.DESCRCTA,
        FIN.CODPARC,
        PAR.NOMEPARC,
		0 AS DEBITO,
		0 AS CREDITO,
        LAN.DTMOV,
        LAN.CODHISTCTB,
        LAN.COMPLHIST,
        LAN.NUMDOC,
        LAN.VENCIMENTO,
        LAN.LIBERADO,
        LAN.CODUSU,
        LAN.CODPROJ,
        LAN.SEQUENCIA

FROM	TCBLAN LAN
INNER JOIN TCBPLA PLA ON PLA.CODCTACTB = LAN.CODCTACTB

-- JOINs adicionais
LEFT JOIN TCBINT IT ON (
    IT.CODEMP = LAN.CODEMP AND
    IT.REFERENCIA = LAN.REFERENCIA AND
    IT.NUMLANC = LAN.NUMLANC AND
    IT.TIPLANC = LAN.TIPLANC AND
    IT.NUMLOTE = LAN.NUMLOTE AND
    IT.SEQUENCIA = LAN.SEQUENCIA
)

INNER JOIN TGFFIN FIN ON (FIN.NUFIN = IT.NUNICO AND IT.ORIGEM = 'F')

LEFT JOIN TGFPAR PAR ON (PAR.CODPARC = FIN.CODPARC)


WHERE	LAN.REFERENCIA < '01/03/2025' --:DTREFERENCIA
AND		LAN.CODEMP = 1 --:CODEMP
AND		LAN.CODCTACTB = 128 --:CODCTACTB
AND		PLA.CTACTB >= '2'
AND		PLA.CTACTB < '3'

GROUP BY 	LAN.CODEMP,
            LAN.REFERENCIA,
            LAN.NUMLANC,
            LAN.NUMLOTE,
            LAN.TIPLANC,
            FIN.CODPARC,
            PAR.NOMEPARC,
			LAN.CODCTACTB,
			PLA.CTACTB,
			PLA.DESCRCTA,
            LAN.DTMOV,
            LAN.CODHISTCTB,
            LAN.COMPLHIST,
            LAN.NUMDOC,
            LAN.VENCIMENTO,
            LAN.LIBERADO,
            LAN.CODUSU,
            LAN.CODPROJ,
            LAN.SEQUENCIA


UNION ALL


SELECT	LAN.CODEMP,
        LAN.REFERENCIA,
        LAN.NUMLOTE,
        LAN.NUMLANC,
        LAN.TIPLANC,
		LAN.CODCTACTB,
		PLA.CTACTB,
		PLA.DESCRCTA,
        FIN.CODPARC,
        PAR.NOMEPARC,
		0 AS DEBITO,
		0 AS CREDITO,
        LAN.DTMOV,
        LAN.CODHISTCTB,
        LAN.COMPLHIST,
        LAN.NUMDOC,
        LAN.VENCIMENTO,
        LAN.LIBERADO,
        LAN.CODUSU,
        LAN.CODPROJ,
        LAN.SEQUENCIA


FROM	TCBLAN LAN
INNER JOIN TCBPLA PLA ON PLA.CODCTACTB = LAN.CODCTACTB


-- JOINs adicionais
LEFT JOIN TCBINT IT ON (
    IT.CODEMP = LAN.CODEMP AND
    IT.REFERENCIA = LAN.REFERENCIA AND
    IT.NUMLANC = LAN.NUMLANC AND
    IT.TIPLANC = LAN.TIPLANC AND
    IT.NUMLOTE = LAN.NUMLOTE AND
    IT.SEQUENCIA = LAN.SEQUENCIA
)

INNER JOIN TGFFIN FIN ON (FIN.NUFIN = IT.NUNICO AND IT.ORIGEM = 'B')

LEFT JOIN TGFPAR PAR ON (
    PAR.CODPARC = FIN.CODPARC
)


WHERE	LAN.REFERENCIA < '01/03/2025' --:DTREFERENCIA
AND		LAN.CODEMP = 1 --:CODEMP
AND		LAN.CODCTACTB = 128 --:CODCTACTB
AND		PLA.CTACTB >= '2'
AND		PLA.CTACTB < '3'

GROUP BY 	LAN.CODEMP,
            LAN.REFERENCIA,
            LAN.NUMLOTE,
            LAN.NUMLANC,
            LAN.TIPLANC,
            FIN.CODPARC,
            PAR.NOMEPARC,
			LAN.CODCTACTB,
			PLA.CTACTB,
			PLA.DESCRCTA,
            LAN.DTMOV,
            LAN.CODHISTCTB,
            LAN.COMPLHIST,
            LAN.NUMDOC,
            LAN.VENCIMENTO,
            LAN.LIBERADO,
            LAN.CODUSU,
            LAN.CODPROJ,
            LAN.SEQUENCIA

UNION ALL

SELECT	LAN.CODEMP,
        LAN.REFERENCIA,
        LAN.NUMLOTE,
        LAN.NUMLANC,
        LAN.TIPLANC,
		LAN.CODCTACTB,
		PLA.CTACTB,
		PLA.DESCRCTA,
        FIN.CODPARC,
        PAR.NOMEPARC,
		NVL(SUM(CASE WHEN LAN.TIPLANC = 'D' THEN LAN.VLRLANC END), 0) AS DEBITO,
		NVL(SUM(CASE WHEN LAN.TIPLANC = 'R' THEN LAN.VLRLANC*-1 END), 0) AS CREDITO,
        LAN.DTMOV,
        LAN.CODHISTCTB,
        LAN.COMPLHIST,
        LAN.NUMDOC,
        LAN.VENCIMENTO,
        LAN.LIBERADO,
        LAN.CODUSU,
        LAN.CODPROJ,
        LAN.SEQUENCIA


FROM	TCBLAN LAN
INNER JOIN TCBPLA PLA ON PLA.CODCTACTB = LAN.CODCTACTB


-- JOINs adicionais
LEFT JOIN TCBINT IT ON (
    IT.CODEMP = LAN.CODEMP AND
    IT.REFERENCIA = LAN.REFERENCIA AND
    IT.NUMLANC = LAN.NUMLANC AND
    IT.TIPLANC = LAN.TIPLANC AND
    IT.NUMLOTE = LAN.NUMLOTE AND
    IT.SEQUENCIA = LAN.SEQUENCIA
)

INNER JOIN TGFFIN FIN ON (FIN.NUFIN = IT.NUNICO AND IT.ORIGEM = 'R')

LEFT JOIN TGFPAR PAR ON (
    PAR.CODPARC = FIN.CODPARC
)

WHERE	LAN.REFERENCIA = '01/03/2025' --:DTREFERENCIA
AND		LAN.CODEMP = 1 --:CODEMP
AND		LAN.CODCTACTB = 128 --:CODCTACTB
AND		PLA.CTACTB >= '2'
AND		PLA.CTACTB < '3'

GROUP BY 	LAN.CODEMP,
            LAN.REFERENCIA,
            LAN.NUMLOTE,
            LAN.NUMLANC,
            LAN.TIPLANC,
            FIN.CODPARC,
            PAR.NOMEPARC,
			LAN.CODCTACTB,
			PLA.CTACTB,
			PLA.DESCRCTA,
            LAN.DTMOV,
            LAN.CODHISTCTB,
            LAN.COMPLHIST,
            LAN.NUMDOC,
            LAN.VENCIMENTO,
            LAN.LIBERADO,
            LAN.CODUSU,
            LAN.CODPROJ,
            LAN.SEQUENCIA



) P

GROUP BY 	P.CODEMP,
            P.REFERENCIA,
            P.NUMLOTE,
            P.NUMLANC,
            P.TIPLANC,
			P.CODPARC,
			P.NOMEPARC,
			P.CODCTACTB,
			P.CTACTB,
			P.DESCRCTA,
            P.DTMOV,
            P.CODHISTCTB,
            P.COMPLHIST,
            P.NUMDOC,
            P.VENCIMENTO,
            P.LIBERADO,
            P.CODUSU,
            P.CODPROJ,
            P.SEQUENCIA

ORDER BY 2
)]]>
		</queryString>
